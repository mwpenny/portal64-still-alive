        "color": {
            "type": "object",
            "properties": {
                "r": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 255
                },
                "g": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 255
                },
                "b": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 255
                },
                "a": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 255
                },
                "bypassEffects": {
                    "type": "boolean"
                }
            },
            "required": [
                "r",
                "g",
                "b"
            ]
