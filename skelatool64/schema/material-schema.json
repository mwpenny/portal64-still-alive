                "gDPSetRenderMode": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/renderMode"
                        },
                        {
                            "type": "array",
                            "items": [{
                                "$ref": "#/definitions/renderMode"
                            }, {
                                "$ref": "#/definitions/renderMode"
                            }]
                        }
                    ]
                },
                "gDPSetCombineMode": {
                    "anyOf": [
                        {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/combineMode"
                            },
                            "maxLength": 2,
                            "minLength": 2
                        },
                        {
                            "$ref": "#/definitions/combineMode"
                        }
                    ]
                },
                "gSPGeometryMode": {
                    "type": "object",
                    "properties": {
                        "clear": {
                            "$ref": "#/definitions/geometryModeArray"
                        },
                        "set": {
                            "$ref": "#/definitions/geometryModeArray"
                        }
                    }
                },
                "gSPTexture": {
                    "$ref": "#/definitions/textureSettings"
                },
                "gDPSetTile": {
                    "oneOf": [
                        {
                            "$ref": "#/definitions/tileSettings"
                        },
                        {
                            "type": "array",
                            "items": {
                                "oneOf": [
                                    {
                                        "type": "null"
                                    },
                                    {
                                        "$ref": "#/definitions/tileSettings"
                                    }
                                ]
                            },
                            "maxLength": 8,
                            "minLength": 1
                        }
                    ]
                },
                "gDPSetPrimColor": {
                    "type": "object",
                    "r": {
                        "type": "number",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "g": {
                        "type": "number",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "b": {
                        "type": "number",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "a": {
                        "type": "number",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "l": {
                        "type": "number",
                        "minimum": 0,
                        "maximum": 255
                    },
                    "m": {
                        "type": "number",
                        "minimum": 0,
                        "maximum": 255
