        "textureSettings": {
            "type": "object",
            "properties": {
                "sc": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 65535
                },
                "tc": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 65535
                },
                "level": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 7
                },
                "tile": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 7
                }
            }
        },
