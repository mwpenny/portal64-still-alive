                    "enum": ["normal", "tangent", "-tangent", "bitangent", "-bitangent"]
                },
                "excludeFromOutput": {
                    "type": "boolean"
                },
                "sortOrder": {
                    "type": "integer"
                },
                "defaultVertexColor": {
                    "$ref": "#/definitions/color"
                }
            }
        }
