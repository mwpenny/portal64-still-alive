                        "siz": {
                            "enum": [
                                "G_IM_SIZ_4b",
                                "G_IM_SIZ_8b",
                                "G_IM_SIZ_16b",
                                "G_IM_SIZ_32b"
                            ]
                        },
                        "tmem": {
                            "type": "number",
                            "minimum": 0,
                            "maximum": 511
                        },
                        "pallete": {
                            "type": "number",
                            "minimum": 0,
                            "maximum": 15
                        },
                        "s": {
                            "$ref": "#/definitions/textureCoordinateSettings"
                        },
                        "t": {
                            "$ref": "#/definitions/textureCoordinateSettings"
                        },
                        "twoTone": {
                            "type": "boolean"
                        },
                        "normalMap": {
                            "type": "boolean"
                        },
                        "invert": {
                            "type": "boolean"
                        },
                        "selectChannel": {
                            "enum": ["r", "g", "b"]
                        },
                        "usePallete": {
                            "type": "string"
                        },
                        "bypassEffects": {
                            "type": "boolean"
                        }
                    },
                    "required": [
                        "filename"
                    ]
                }
            ]
