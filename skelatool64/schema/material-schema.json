                {
                    "type": "object",
                    "properties": {
                        "filename": {
                            "type": "string"
                        },
                        "fmt": {
                            "enum": [
                                "G_IM_FMT_RGBA",
                                "G_IM_FMT_YUV",
                                "G_IM_FMT_CI",
                                "G_IM_FMT_I",
                                "G_IM_FMT_IA"
