                    "$ref": "#/definitions/color"
                },
                "gDPSetFogColor": {
                    "$ref": "#/definitions/color"
                },
                "gDPSetBlendColor": {
                    "$ref": "#/definitions/color"
                },
                "properties": {
                    "type": "object",
                    "additionalProperties": {
                        "oneOf": [{
